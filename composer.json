{
  "name": "behinddesign/craft-object-storage",
  "description": "A Craft 4 plugin that integrates with most S3 compatible Object Storage. Includes custom host / path or subdomain connection types",
  "type": "craft-plugin",
  "keywords": [
    "craftcms",
    "flysystem",
    "object-storage",
    "plugin",
    "s3"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Behind Design",
      "homepage": "https://www.behinddesign.co.uk/"
    }
  ],
  "require": {
    "php": "^8.0.2",
    "craftcms/cms": "^4.0.0",
    "craftcms/flysystem": "^1.0.0",
    "league/flysystem-aws-s3-v3": "^3.0.0",
    "symfony/console": "^5.1"
  },
  "require-dev": {
    "craftcms/phpstan": "dev-main",
    "craftcms/rector": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "behinddesign\\ObjectStorage\\": "src/"
    }
  },
  "extra": {
    "name": "Generic Object Storage Volume",
    "handle": "behinddesign-object-storage",
    "changelogUrl": "https://raw.githubusercontent.com/behinddesign/craft-object-storage/master/CHANGELOG.md",
    "downloadUrl": "https://github.com/behinddesign/craft-object-storage/archive/master.zip"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true,
      "pestphp/pest-plugin": true
    }
  }
}
